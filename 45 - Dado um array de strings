    "cruzeiro",
  ]
    const object = {}
    randomStrings.forEach((string) => {
    if (object[string]) {
      object[string]++
    } else {
      object[string] = 1
    }
  })
    console.log("Lista de times campeões da Copa do Brasil")
    console.table(object)
